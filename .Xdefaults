!Xft.dpi: 96
Xft.rgba: rgb
!Xcursor.theme_core:     true
!!!Font
URxvt.font:     -*-terminus-*-*-*-*-12-*-*-*-*-*-iso10646-1
!URxvt.boldFont:  -misc-fixed-medium-r-*-*-10-*-*-*-*-*-iso10646-1

!URxvt.font: xft:terminus-8
!URxvt.font: xft:Envy Code R-10
!URxvt.boldFont: xft:terminus2-8
!!!Settings
URxvt*background:grey10
URxvt*cursorColor:#ffffff
URxvt*name: Terminal
URxvt*title: Terminal
URxvt.geometry: 100x35+30+100
URxvt.scrollBar: false
URxvt.xftAntialias: true
!URxvt*depth:                32
!URxvt*background:        rgba:0000/0000/0000/dddd
URxvt*perl-ext-common: default,matcher
URxvt.matcher.rend.0: Uline Bold fg8 bg1
URxvt*urlLauncher: chromium
URxvt*matcher.button: 1
! grey
URxvt.tabbed.tabbar-fg: 8 
! ! black
URxvt.tabbed.tabbar-bg: #000
! ! white
URxvt.tabbed.tab-fg:    7
! ! blue
URxvt.tabbed.tab-bg:    4
!Urxvt.inheritPixmap: true
!URxvt.transparent: true
!URxvt.tintColor:white
!URxvt.shading: 15 
URxvt.jumpScroll: True
URxvt.scrollstyle: plain
URxvt.loginShell: False
URxvt.scrollBar_floating: False
URxvt.scrollTtyOutput: False
URxvt.scrollTtyKeypress: True
URxvt.scrollWithBuffer: True
URxvt.saveLines: 5000
URxvt.internalBorder: 0
URxvt.externalBorder: 0
URxvt.cursorBlink: True
URxvt.visualBell: False
URxvt.urgentOnBell:  True
!URxvt.cursorUnderline: boolean
!URxvt.borderLess:	true

