Xcursor.theme: simpleandsoft
!Xcursor.size:  16 
!Xft.dpi:100
!Xcursor.theme_core:     true
Xft.antialias:  1
Xft.hinting:    1
Xft.hintstyle:  hintslight
Xft.rgba:       rgb
!!!Font
!URxvt.font:     -*-terminus-*-*-*-*-12-*-*-*-*-*-iso10646-1
!URxvt.boldFont:  -misc-fixed-medium-r-*-*-10-*-*-*-*-*-iso10646-1

URxvt.font: xft:Lucida Console:pixelsize=12
!URxvt.boldFont: xft:Lucida Console:pixelsize=12
!!!Settings
!URxvt*background:grey10
URxvt*cursorColor:#ffffff
URxvt*name: Terminal
URxvt*title: Terminal
URxvt.geometry: 100x35+30+100
URxvt.scrollBar: false
URxvt.xftAntialias: true
!URxvt*depth:                32
!URxvt*background:        rgba:0000/0000/0000/dddd
URxvt*perl-ext-common: default,matcher
URxvt*urlLauncher: chromium-bin
URxvt*matcher.button: 1
Urxvt.inheritPixmap: true
!URxvt.transparent: true
!URxvt.tintColor:white
!URxvt.shading: 15 
URxvt.jumpScroll: True
URxvt.scrollstyle: plain
URxvt.loginShell: False
URxvt.scrollBar_floating: False
URxvt.scrollTtyOutput: False
URxvt.scrollTtyKeypress: True
URxvt.scrollWithBuffer: True
URxvt.saveLines: 5000
URxvt.internalBorder: 0
URxvt.externalBorder: 0
URxvt.cursorBlink: True
URxvt.visualBell: False
URxvt.urgentOnBell:  true
!URxvt.borderLess:	true

!
!!-------------------------------------
!!! options (xterm)
!!!-------------------------------------
!
xterm*termName:                 xterm
xterm*locale:                   true
xterm*utf8:                     1
xterm*eightBitInput:            false
xterm*metaSendsEscape:          true
!xterm*scrollTtyOutput:          false
!
xterm*urgentOnBell: true
xterm*boldMode:                 false
!xterm*font:                                 -misc-fixed-medium-r-*-*-10-*-*-*-*-*-iso10646-1
!xterm*boldFont:                     -misc-fixed-medium-r-*-*-10-*-*-*-*-*-iso10646-1
!xterm*font:                    -*-lucida console-*-*-*-*-10-*-*-*-*-*-iso10646-1
!xterm*boldFont:                -*-lucida console-*-*-*-*-10-*-*-*-*-*-iso10646-1
xterm*faceName:     Lucida Console
xterm*faceSize:     10
!
xterm*geometry:                 100x30
!
