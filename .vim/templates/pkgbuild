pkgname=
pkgver=
pkgrel=1
pkgdesc=""
arch=('i686' 'x86_64')
url=""
license=('GPL')
depends=()
makedepends=()
provides=()
conflicts=()

source=($pkgname-$pkgver.tar.gz)
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"

  ./configure --prefix=/usr
  make || return 1
  make DESTDIR="$pkgdir/" install
}

